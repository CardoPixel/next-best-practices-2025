{
    "printWidth": 80,
    "tabWidth": 4,
    "trailingComma": "all",
    "singleQuote": false,
    "semi": true,
    "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-imports"],
    "importOrder": [
        "^@core/(.*)$",
        "",
        "^@server/(.*)$",
        "^@ui/(.*)$",
        "",
        "^[./]"
    ]
}
